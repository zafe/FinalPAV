<Window x:Class="FinalPAV.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FinalPAV"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel HorizontalAlignment="Left" Height="100" Margin="0,10,0,0" VerticalAlignment="Top" Width="792">
            <Label x:Name="label" Content="Usuario"/>
            <TextBox x:Name="textBox" Height="23" TextWrapping="Wrap"/>
            <Label x:Name="label1" Content="Contraseña"/>
            <TextBox x:Name="textBox1" Height="23" TextWrapping="Wrap" TextChanged="TextBox1_TextChanged" AutomationProperties.HelpText="fsadfas"/>
        </StackPanel>

    </Grid>
</Window>
